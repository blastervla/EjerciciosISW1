	| game |
	
	game := TerniLapilli new.
	game putXAt: 1@1.
	game putOAt: 2@1.	
	
	self assert: (game XTokenAt: 1@1).
	self assert: (game OTokenAt: 2@1).
	self deny: (game XTokenAt: 2@1).! !
